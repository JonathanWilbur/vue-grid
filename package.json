{
  "name": "vue-grid-layout",
  "version": "2.1.13",
  "description": "A draggable and resizable grid layout, as a Vue component.",
  "keywords": [
    "grid",
    "vuejs",
    "drag",
    "draggable",
    "resize",
    "resizable",
    "fluid",
    "responsive"
  ],
  "main": "dist/vue-grid-layout.min.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "prod": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "webpack",
    "dist": "webpack --progress --hide-modules --config webpack.build.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.build.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbaysolutions/vue-grid-layout.git"
  },
  "author": "Gustavo Santos (JBay Solutions) <gustavo.santos@jbaysolutions.com> (http://www.jbaysolutions.com)",
  "dependencies": {
    "@types/element-resize-detector": "^1.1.0",
    "element-resize-detector": "^1.1.14",
    "interactjs": "^1.3.4",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "ts-loader": "^4.3.1",
    "typescript": "^2.9.1",
    "vue-class-component": "^6.2.0",
    "vue-loader": "^15.2.4",
    "vue-property-decorator": "^6.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuex-typex": "^3.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "license": "MIT"
}
